{
  "extends": ["eslint:recommended"],
  "env": {
    "browser": true
  },
  "globals": {
    "__DEVELOPMENT__": true,
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": { }
  },
  "plugins": [
  ],
  "rules": {
    "valid-jsdoc": "error",
    "react/jsx-uses-react": "warn",
    "react/jsx-no-undef": "error",
    "react/wrap-multilines": "error",
    "comma-dangle": ["error", "always-multiline"]
  }
}
